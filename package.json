{
  "name": "apac-revenue-projections",
  "version": "2.3.0-analysis-refresh",
  "description": "APAC Revenue Projections System - Financial modeling tool for digital authentication services",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "watch": "vite build --watch",
    "watch:full": "chokidar 'index-working.html' 'index.html' 'styles.css' 'app.js' 'demographics/**/*.json' -c 'npm run build'",
    "dev:auto": "npm run build && npm run watch",
    "dev:preview": "npm run build && concurrently \"npm run watch\" \"npm run preview\"",
    "test": "jest --config=jest.config.cjs",
    "test:unit": "jest --config=jest.config.cjs --selectProjects unit/integration",
    "test:integration": "jest --config=jest.config.cjs tests/integration",
    "test:e2e": "jest --config=jest.config.cjs --selectProjects e2e",
    "test:browser": "HEADLESS=false npm run test:e2e",
    "test:e2e:visible": "HEADLESS=false npm run test:e2e",
    "test:browser:safe": "HEADLESS=false timeout 30s npm run test:e2e || npm run kill-chrome",
    "test:single": "HEADLESS=false npx jest --config=jest.config.cjs tests/e2e/app.test.js --testNamePattern='should load the application' --maxWorkers=1",
    "test:e2e:debug": "HEADLESS=false SLOWMO=100 DEVTOOLS=true DEBUG_LOGS=true npm run test:e2e",
    "test:e2e:step": "HEADLESS=false SLOWMO=500 PAUSE_AFTER_EACH=true DEBUG_BREAKPOINTS=true npm run test:e2e",
    "test:coverage": "jest --config=jest.config.cjs --coverage",
    "test:watch": "jest --config=jest.config.cjs --watch",
    "test:all": "node tests/run-all-tests.js",
    "test:report": "node tests/coverage-report.js",
    "kill-chrome": "node kill-chrome.cjs",
    "test:playwright": "playwright test",
    "test:playwright:edge": "playwright test --project=edge",
    "test:playwright:safari": "playwright test --project=safari",
    "test:playwright:chrome": "playwright test --project=chrome",
    "test:playwright:headed": "HEADLESS=false playwright test",
    "test:playwright:headed:edge": "HEADLESS=false playwright test --project=edge",
    "test:playwright:headed:safari": "HEADLESS=false playwright test --project=safari",
    "test:playwright:report": "playwright show-report"
  },
  "keywords": [
    "financial-modeling",
    "revenue-projections",
    "aadhaar",
    "apac",
    "authentication",
    "analytics"
  ],
  "author": "Mastercard Product Management",
  "license": "SEE LICENSE IN LICENSE",
  "devDependencies": {
    "@playwright/test": "^1.55.1",
    "chokidar-cli": "^3.0.0",
    "concurrently": "^9.2.1",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-puppeteer": "^9.0.0",
    "playwright": "^1.55.1",
    "puppeteer": "^21.0.0",
    "vite": "^5.4.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
